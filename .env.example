# Expo/Metro (optional)
EXPO_UNSTABLE_LIVE_BINDINGS=true

# Local server (AASA/assetlinks)
PORT=8787

# iOS
IOS_APP_ID=""                 # e.g., com.yelo.noshDelivery
IOS_TEST_DEVICE_UUID=""       # optional; for ad-hoc/internal installs

# Your /server deployment domain (domain only, no protocol)
EXPO_PUBLIC_SERVER_DOMAIN=""  # e.g., app.example.com or dev.example.com

# Expo debug output
EXPO_DEBUG=true

# Single strategy for tunnels (full URL, includes protocol)
# Example: https://5650e3201c52.ngrok-free.app
EXPO_TUNNEL_URL=""

# Server-only secret (do not include in client runtime)
# node --eval "console.log(require('node:crypto').randomBytes(32).toString('hex'))"
SIWE_SECRET=""

# Porto base URL (use local/staging Relay if self-hosting)
EXPO_PUBLIC_PORTO_BASE_URL="https://id.porto.sh"

# App scheme (must match app.config.ts)
EXPO_PUBLIC_SCHEME="porto-rn"

# Porto dialog host (optional; not required for current setup)
EXPO_PUBLIC_PORTO_HOST=""